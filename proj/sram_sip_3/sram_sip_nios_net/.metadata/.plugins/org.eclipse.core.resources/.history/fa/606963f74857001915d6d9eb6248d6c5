typedef struct
{
    INT32U cmd;
    INT32U addr;
    INT32U data;
    INT32U area;
    INT32U inc_dec;
    INT32U cycle;
    INT32U jump;
} CMD_PACKAGE; 

INT32U buf_int (INT8U* buf)
{
    int i=0;
    INT32U number=0;
    for(i=0;i<sizeof(INT32U);i++)
    {       
        number |= ((INT32U)(*(buf+i))<<(i<<3));//little-endian   
    }
    return number;
}



